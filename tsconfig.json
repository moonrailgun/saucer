{
  "compilerOptions": {
    "strict": true,
    "incremental": true,
    "noImplicitAny": true,
    "target": "es5",
    "module": "esnext",
    "lib": ["es2016", "es2015", "es2017", "dom"],
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "experimentalDecorators": true,
    "importsNotUsedAsValues": "error",
    "skipLibCheck": true
  }
}
