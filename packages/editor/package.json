    "@saucer/core": "^0.0.0",
    "classnames": "^2.2.6",
    "less": "^3.12.2",
    "rc-tree": "^4.0.0-beta.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3"
  }
